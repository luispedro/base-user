\mode<presentation>


\definecolor{cmudarkred}{rgb}{0.72,0.15,0.20}
\definecolor{cmulightred}{rgb}{0.72,0.15,0.20}
\definecolor{cmutitle}{rgb}{0.36,.07,.10}


\setbeamercolor{palette primary}{bg=cmudarkred}
\setbeamercolor{palette secundary}{bg=cmudarkred}
\setbeamercolor{palette tertiary}{bg=cmudarkred,fg=white}
\setbeamercolor{titlelike}{bg=cmulightred,fg=white}
\setbeamercolor{title}{bg=cmulightred!70!black,fg=white}


\setbeamercolor{section number projected}{bg=cmudarkred}
\setbeamercolor{section in toc}{fg=cmudarkred}

\setbeamercolor{item projected}{bg=cmudarkred}
\setbeamercolor{subitem projected}{parent=item projected}

\setbeamercolor{block title}{parent=titlelike}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!25!bg}

\mode
<all>

